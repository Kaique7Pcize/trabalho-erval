# Função para ler um vetor de 10 posições
def ler_vetor():
    vetor = []
    for i in range(10):
        elemento = int(input("Digite o valor para a posição {}: ".format(i)))
        vetor.append(elemento)
    return vetor

# Função para encontrar o maior e o menor elemento do vetor
def encontrar_maior_menor(vetor):
    maior = max(vetor)
    menor = min(vetor)
    return maior, menor

# Ler o vetor de 10 posições
vetor = ler_vetor()

# Encontrar o maior e o menor elemento do vetor
maior, menor = encontrar_maior_menor(vetor)

# Imprimir o resultado
print("O maior elemento do vetor é:", maior)
print("O menor elemento do vetor é:", menor)
